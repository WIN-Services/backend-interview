{
  "name": "win-order-service",
  "version": "1.0.0",
  "main": "./src/app.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon ./src/app.ts",
    "migration:init": "sh -c 'knex init -x ts' ",
    "migration:createTable": "sh -c 'knex migrate:make ${0} -x ts' ",
    "migration:migrate": "sh -c 'knex migrate:latest' ",
    "migration:rollback": "sh -c 'knex migrate:rollback' ",
    "migration:rollback:all": "sh -c 'knex migrate:rollback --all' ",
    "lint": "eslint src --cache --ext .ts",
    "test": "jest --runInBand --detectOpenHandles .test.ts"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.4.0",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.1",
    "nodemon": "^3.0.1",
    "pg": "^8.6.0",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/koa": "^2.13.12",
    "@types/koa-router": "^7.4.8",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
